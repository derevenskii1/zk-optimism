{
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer",
        "editor.formatOnSave": true,
        "editor.hover.enabled": true
    },
    "rust-analyzer.check.overrideCommand": [
        "cargo",
        "clippy",
        "--workspace",
        "--message-format=json",
        "--all-features",
        "--all-targets",
        "--",
        "-A",
        "incomplete-features"
    ],
    "editor.rulers": [
        100
    ]
}